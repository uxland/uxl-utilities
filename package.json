{
    "name": "@uxland/uxl-utilities",
    "version": "0.0.0-development",
    "description": "UXLand - Utilities",
    "homepage": "https://github.com/uxland/uxl-utilities#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/uxland/uxl-utilities.git"
    },
    "bugs": {
        "url": "https://github.com/uxland/uxl-utilities/issues"
    },
    "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "keywords": [
        "polymer",
        "web components"
    ],
    "devDependencies": {
        "@polymer/iron-demo-helpers": "^3.0.0-pre.19",
        "@types/chai": "^4.1.4",
        "@types/lodash-es": "^4.17.0",
        "@types/mocha": "^5.2.2",
        "@types/redux-mock-store": "^1.0.0",
        "@types/sinon": "^5.0.1",
        "@webcomponents/custom-elements": "^1.2.0",
        "@webcomponents/webcomponentsjs": "^2.0.0",
        "chai": "^4.1.2",
        "dts-generator": "^2.1.0",
        "esm": "^3.0.66",
        "gulp": "^3.9.1",
        "gulp-run": "^1.7.1",
        "jsdom": "^11.11.0",
        "lodash": "3.10.1",
        "mocha": "^5.2.0",
        "mochawesome": "^3.0.2",
        "mutation-observer": "^1.0.3",
        "ncp": "^2.0.0",
        "nodemon": "^1.18.3",
        "nsp": "^2.6.3",
        "path": "^0.12.7",
        "raf": "^3.4.0",
        "require-dir": "^0.3.2",
        "rimraf": "^2.6.2",
        "semantic-release": "^15.9.3",
        "sinon": "^6.0.0",
        "travis-deploy-once": "^5.0.1",
        "typescript": "^3.0.3",
        "wct-browser-legacy": "^1.0.0"
    },
    "dependencies": {
        "@polymer/lit-element": "^0.6.1"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
        "test:ui": "polymer test --config-file wct.config.json",
        "test:unit": "npm run build && node --experimental-modules node_modules/mocha/bin/_mocha -r esm ./test/unit/setup.js --ui tdd --reporter mochawesome test/unit --recursive",
        "dist": "tsc --p gulp/tsconfig-dist.json && gulp generate-d-ts",
        "tdd": "node --experimental-modules node_modules/mocha/bin/_mocha --watch -r esm ./test/unit/setup.js --ui tdd --reporter mochawesome test/unit --recursive",
        "prepublishOnly": "if [ -d dist ]; then ncp dist/. . && rimraf dist/; fi",
        "travis-deploy-once": "travis-deploy-once",
        "semantic-release": "semantic-release"
    },
    "license": "Apache-2.0",
    "directories": {
        "test": "test"
    },
    "publishConfig": {
        "access": "public"
    }
}
