{
    "name": "@uxland/uxl-utilities",
    "version": "0.0.0-development",
    "description": "UXLand - Utilities",
    "homepage": "https://github.com/uxland/uxl-utilities#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/uxland/uxl-utilities.git"
    },
    "bugs": {
        "url": "https://github.com/uxland/uxl-utilities/issues"
    },
    "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
    "main": "lib/index.js",
    "module": "index.js",
    "types": "lib/index.d.ts",
    "keywords": [
        "polymer",
        "web components"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
        "@babel/plugin-transform-modules-commonjs": "^7.2.0",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-typescript": "^7.3.3",
        "@types/jest": "^24.0.11",
        "@types/ramda": "^0.26.3",
        "@types/redux-mock-store": "^1.0.0",
        "@webcomponents/custom-elements": "^1.2.0",
        "@webcomponents/webcomponentsjs": "^2.0.0",
        "babel-cli": "^6.26.0",
        "cross-env": "^5.2.0",
        "jest": "^24.5.0",
        "rollup": "^1.6.0",
        "rollup-plugin-typescript": "^1.0.0",
        "rollup-plugin-uglify": "^6.0.2",
        "semantic-release": "^15.9.3",
        "travis-deploy-once": "^5.0.1",
        "ts-jest": "^24.0.0",
        "tslib": "^1.9.3",
        "typescript": "^3.3.3333"
    },
    "dependencies": {
        "lit-element": "^2.0.0-rc.5",
        "ramda": "^0.26.1"
    },
    "scripts": {
        "build:es": "cross-env BABEL_ENV=es babel --extensions '.ts' src --out-dir es",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir lib",
        "build:types": "tsc --emitDeclarationOnly --declaration --rootDir src --outDir lib",
        "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/uxl-utilities.js",
        "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/uxl-utilities.min.js",
        "build": "npm run build:es && npm run build:cjs && npm run build:types",
        "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
        "test:ui": "polymer test --config-file wct.config.json",
        "test:unit": "node --experimental-modules node_modules/mocha/bin/_mocha -r esm ./test/unit/setup.js --ui tdd --reporter mochawesome test/unit/**/*.loader.js",
        "dist": "npm run build",
        "tdd": "node --experimental-modules node_modules/mocha/bin/_mocha --watch -r esm ./test/unit/setup.js --ui tdd --reporter mochawesome test/unit/**/*.loader.js",
        "travis-deploy-once": "travis-deploy-once",
        "semantic-release": "semantic-release"
    },
    "license": "Apache-2.0",
    "directories": {
        "test": "test"
    },
    "publishConfig": {
        "access": "public"
    }
}
